Store } from \"vuex\";\n\nexport default {\n  name: \"HomeView\",\n  components: {\n    ModalComp,\n  },\n\n  setup() {\n    // console.log(this.$store);\n\n    const store = useStore();\n\n    const modalActive = ref(false);\n\n    const connect = async () => {\n      await store.commit(\"connectWallet\");\n      await console.log(store.caver);\n      toggleModal();\n    };\n\n    const toggleModal = () => {\n      modalActive.value = !modalActive.value;\n    };\n\n    return { modalActive, connect, toggleModal };\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.title {\n  height: 20vh;\n}\n\n.home {\n  height: 80vh;\n}\n\n.title,\n.home {\n  background-color: rgba(0, 176, 235, 0.5);\n  display: flex;\n  justify-content: center;\n  align-items: center;\n\n  .modal-content {\n    display: flex;\n    flex-direction: column;\n\n    input {\n      position: relative;\n    }\n\n    h1,\n    p {\n      margin-bottom: 16px;\n    }\n\n    h1 {\n      font-size: 32px;\n    }\n\n    p {\n      font-size: 18px;\n    }\n  }\n}\n</style>"]},"metadata":{},"sourceType":"module"}